# Arquivo de ambiente para produção
# Valores que devem ser substituídos em produção

# === CONFIGURAÇÕES BÁSICAS ===
NODE_ENV=production
REACT_APP_VERSION=0.1.0

# === AUTENTICAÇÃO ===
# Em produção, defina SKIP_AUTH como false
REACT_APP_SKIP_AUTH=false
# Configure um Google Client ID válido
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here

# === API E WEBSOCKET ===
# Configure URLs de produção
REACT_APP_API_URL=https://api.priceguard.com
REACT_APP_WS_URL=wss://api.priceguard.com/ws/dashboard

# === FEATURES FLAGS ===
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_DEBUG_MODE=false

# === CONFIGURAÇÕES DE PRODUÇÃO ===
GENERATE_SOURCEMAP=false
